pipeline {
    agent any 
    stages {
    stage ('pull'){
        steps {
            git credentialsId: 'github', url: 'https://github.com/oselukar/vue-starter.git'
            
        } 
                   }
 
 stage ('Docker-Build') {
     steps {
         sh 'docker build -t omkesh/vue-starter:v${BUILD_NUMBER} .'
         
     }
 }
stage ('docker push'){
     steps { 
         withDockerRegistry([credentialsId: 'dockerhub' , url: ""]){
             sh 'docker push omkesh/vue-starter:v${BUILD_NUMBER}-${GIT-BRANCH}'
         }
     }
    }
}
}
